    "@types/node": "^12.11.1",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.1.0",
    "ng-packagr": "^16.2.3",
    "ts-jest": "^29.1.0",
    "typescript": "~4.9.5"
  }
